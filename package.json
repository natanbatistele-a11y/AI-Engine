{
    "name": "engine-ia-frontend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "preflight": "tsx scripts/preflightEnv.ts",
        "dev": "npm run preflight && concurrently -k \"npm:server:dev\" \"vite\"",
        "dev:client": "vite",
        "server:dev": "tsx watch server/index.ts",
        "check:keys": "tsx scripts/checkApiKeys.ts",
        "build": "tsc -b && vite build",
        "start": "node dist/server/index.js",
        "preview": "vite preview",
        "lint": "tsc --noEmit"
    },
    "dependencies": {
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "framer-motion": "^11.0.0",
        "lucide-react": "^0.408.0",
        "openai": "^6.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/node": "^24.10.0",
        "@types/react": "^18.2.48",
        "@types/react-dom": "^18.2.18",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "autoprefixer": "^10.4.16",
        "concurrently": "^9.2.1",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.3",
        "tsx": "^4.20.6",
        "typescript": "^5.4.3",
        "vite": "^7.2.4"
    }
}
